CREATE VIEW consumo_individual AS
SELECT
  despesa_individual.seq,
  despesa_individual.UF,
  despesa_individual.renda_total,
  despesa_individual.num_uc,
  despesa_individual.V9001 AS tipo_despesa,
  despesa_individual.V8000 AS valor_despesa,
  despesa_individual.V9004 AS local_aquisicao,
  consumo_alimentar_pof.v9015 AS horario_consumo,
  consumo_alimentar_pof.v9016 AS forma_preparacao,
  consumo_alimentar_pof.v9017 AS ocasiao_consumo,
  consumo_alimentar_pof.v9018 AS local_refeicao,
  consumo_alimentar_pof.DIA_SEMANA,
  consumo_alimentar_pof.DIA_ATIPICO
FROM
  despesa_individual
JOIN
  consumo_alimentar_pof
ON
  despesa_individual.seq = consumo_alimentar_pof.seq;